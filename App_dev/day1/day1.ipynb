    "def func1(n):\n",
    "    if n%2 == 0:\n",
    "        return 'even'\n",
    "    else:\n",
    "        return 'odd'\n",
    "\n",
    "n = int(input())\n",
    "str1 = func1(n)\n",
    "print(str1)"
   ]
